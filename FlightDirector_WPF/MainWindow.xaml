<Window x:Class="FlightDirector_WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:scm="clr-namespace:System.ComponentModel;assembly=WindowsBase"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        xmlns:local="clr-namespace:FlightDirector_WPF"
        Loaded="Window_Loaded"
        mc:Ignorable="d"
        Title="ISS Flight Director" Height="570" Width="1005"
        Background="Black" Foreground="SeaGreen">
    <Window.Resources>
        <local:FlightViewModel x:Key="dd"/>
        <CollectionViewSource x:Key="cv" Source="{Binding Source={StaticResource dd}}" >
            <CollectionViewSource.GroupDescriptions>
                <PropertyGroupDescription PropertyName="System"/>
            </CollectionViewSource.GroupDescriptions>

        </CollectionViewSource>
    </Window.Resources>

    <Grid DataContext="{StaticResource dd}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="69*"/>
            <ColumnDefinition Width="5"/>
            <ColumnDefinition Width="78*"/>
            <!--<ColumnDefinition Width="200"/>-->
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="2*"/>
        </Grid.RowDefinitions>
        <DataGrid
            ItemsSource="{Binding Source={StaticResource cv}}"
            AutoGenerateColumns="False" Grid.RowSpan="2"
            Foreground="SeaGreen" Background="Black" RowBackground="Black"
            SelectionUnit="Cell">

            <DataGrid.GroupStyle>
                <GroupStyle>
                    <GroupStyle.ContainerStyle>
                        <Style TargetType="{x:Type GroupItem}">
                            <Setter Property="Margin" Value="0,0,0,0"/>
                            <Setter Property="Template">
                                <Setter.Value>
                                    <ControlTemplate TargetType="{x:Type GroupItem}">
                                        <Expander IsExpanded="False" Foreground="SeaGreen">
                                            <!--BorderBrush="DarkSeaGreen" Foreground="SeaGreen" BorderThickness="0,0,0,0.5">-->

                                            <Expander.Header>
                                                <DockPanel>
                                                    <TextBlock FontWeight="Bold" Text="{Binding Path=Name}" Margin="5,0,0,0" Width="100"/>
                                                    <TextBlock FontWeight="Bold" Text="{Binding Path=ItemCount}"/>
                                                </DockPanel>
                                            </Expander.Header>
                                            <Expander.Content>
                                                <ItemsPresenter Margin="22,0,0,0" />
                                            </Expander.Content>
                                        </Expander>
                                    </ControlTemplate>
                                </Setter.Value>
                            </Setter>
                        </Style>
                    </GroupStyle.ContainerStyle>
                </GroupStyle>
            </DataGrid.GroupStyle>

            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Id}" Header="TelemetryId" IsReadOnly="True" CanUserReorder="False"/>
                <DataGridTextColumn Binding="{Binding Description}" Header="Description" IsReadOnly="True"/>
                <DataGridTextColumn Binding="{Binding TranslatedValue}" Header="Value" IsReadOnly="True"/>
            </DataGrid.Columns>
        </DataGrid>
        <GridSplitter Grid.Column="1" Grid.RowSpan="2" HorizontalAlignment="Stretch" Margin="0,0,5,0"/>
        <Grid Grid.Column="2" Grid.RowSpan="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="2*"/>
            </Grid.RowDefinitions>

            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="7*"/>
                </Grid.RowDefinitions>
                <Button Content="reload" Click="Iss_cam_reload"/>
                <Button Content="popout" Click="Iss_cam_popout" Grid.Column="1"/>
                <wv2:WebView2 x:Name="iss_cam"  Grid.Row="1" Grid.ColumnSpan="2" Source="https://ustream.tv/embed/9408562?autoplay=true"/>
            </Grid>

            <Grid Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="7*"/>
                </Grid.RowDefinitions>
                <Button Content="reload" Click="EHDC_reload"/>
                <Button Content="popout" Click="EHDC_popout" Grid.Column="1"/>
                <wv2:WebView2 x:Name="ehd_cam" Grid.Row="1" Grid.ColumnSpan="2" Source="https://ustream.tv/embed/17074538?autoplay=true"/>
            </Grid>

            <DockPanel Grid.ColumnSpan="2" Grid.Row="1" LastChildFill="True">
                <ListBox ItemsSource="{Binding Path=Log}" Foreground="SeaGreen" Background="Black" SourceUpdated="ListBox_SourceUpdated"/>
            </DockPanel>


        </Grid>
